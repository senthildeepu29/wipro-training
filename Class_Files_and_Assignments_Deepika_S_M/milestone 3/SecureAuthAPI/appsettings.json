{
  "JwtSettings": {
    "Issuer": "SecureAuthAPI",
    "Audience": "SecureAuthAPIUsers",
    "SecretKey": "YourVeryStrongSecretKeyHere_ChangeThisToSomethingSecure",
    "TokenExpiryInMinutes": 60
  },
  "Authentication": {
    "Google": {
      "ClientId": "YourGoogleClientIdHere",
      "ClientSecret": "YourGoogleClientSecretHere"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=SecureAuthDb;Trusted_Connection=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
